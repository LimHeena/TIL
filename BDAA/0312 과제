  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "a9e64f00",
   "metadata": {},
   "outputs": [],
   "source": [
    "parts = ['Noun'] #명사 추출\n",
    "all_words_df = pd.DataFrame()\n",
    "for i in range(len(df_ljm)):\n",
    "    text = str(df_ljm.iloc[i])\n",
    "    words = twt.pos(text)\n",
    "    words_df = pd.DataFrame()\n",
    "    for n in words:\n",
    "        if n == 'EOS' or n == \"\" or len(n[0]) == 1: #길이가 1개인 것도 제외\n",
    "            continue\n",
    "        word_tmp = n[0]\n",
    "        part = n[1]\n",
    "        if not (part in parts):\n",
    "            continue\n",
    "        words_df[word_tmp]=[1]\n",
    "    all_words_df = pd.concat([all_words_df, words_df], ignore_index =True)    "
   ]
